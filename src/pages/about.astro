---
// import '../styles/global.css';
import BaseLayout from '../layouts/BaseLayout.astro';

const pageTitle = "About";
---
<BaseLayout pageTitle={pageTitle}>
  <section class="page-header">
    <h1 class="h1-default">{pageTitle}</h1>
  </section>

  <section class="mb-20 text-center">
    <div class="max-w-xl px-5 mx-auto my-0">
      <p class="mb-5">We are a Military Veteran owned business, teaming up Active Military, Military Veterans, and members of the Disabled Community, to provide a high quality cleaning service for your home or commercial facility.</p>
      <p>Based out of San Diego, we are in the process of creating a way for our services to expand country wide.</p>
    </div>
  </section>
</BaseLayout>