---
// import '../styles/global.css';
import BaseLayout from '../layouts/BaseLayout.astro';

const pageTitle = "Partners";
---
<BaseLayout pageTitle={pageTitle}>
  <section class="page-header">
    <h1 class="h1-default">{pageTitle}</h1>
  </section>

  <section class="mb-20 text-center">
    <div class="flex justify-center max-w-4xl px-5 mx-auto my-0">
      <div class="max-w-[250px] m-5 border-2 p-3">
        <div class="flex justify-center p-2 mb-3 align-middle bg-gray-100 border-2">
          <img class="h-auto max-w-full" src="/partners/motivatedmilitarymovers.png" />
        </div>
        <div class="text-xs">
          <p class="mb-3"><span class="font-bold">Motivated Military Movers</span> is Veteran Owned business founded by Terry Wagner in 2008. You will not find a higher quality moving service anywhere in the San Diego County area.</p>
          <a class="text-blue-600 hover:underline" target="_blank" href="https://www.motivatedmilitarymovers.com">Visit MMM</a>
        </div>
      </div>
    </div>
  </section>
</BaseLayout>