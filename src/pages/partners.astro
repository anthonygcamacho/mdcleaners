---
// import '../styles/global.css';
import BaseLayout from '../layouts/BaseLayout.astro';

const pageTitle = "Partners";
---
<BaseLayout pageTitle={pageTitle}>
  <section class="page-header">
    <h1 class="h1-default">{pageTitle}</h1>
  </section>

  <section class="mb-20 text-center">
    <div class="flex justify-center max-w-4xl px-5 mx-auto my-0">
      <div class="max-w-[250px] m-5 border-2 p-2">
        <div>
          <img src="/partners/motivatedmilitarymovers.png" />
        </div>
        <div>
          <p class="mb-5">We are a Military Veteran owned business, teaming up Active Military, Military Veterans, and members of the Disabled Community, to provide a high quality cleaning service for your home or facility.</p>
          <p>Based out of San Diego, we are in the process of creating a way for our services to expand country wide.</p>
        </div>
      </div>
    </div>
  </section>
</BaseLayout>